{
  "name": "my-backpack-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@skyscanner/backpack-react-scripts": "9.4.0",
    "jquery": "^3.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyz": "npx source-map-explorer@1.8.0 'build/static/js/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bpk-component-button": "^3.4.7",
    "bpk-component-code": "^2.1.7",
    "bpk-component-image": "^5.0.16",
    "bpk-component-text": "^3.2.7",
    "bpk-mixins": "^20.2.7",
    "bpk-stylesheets": "^6.1.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-native": "^0.66.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "backpack-react-scripts": {
    "enableAutomaticChunking": true,
    "vendorsChunkRegex": "[\\/]node_modules[\\/]"
  }
}
